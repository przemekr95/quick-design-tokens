{
  "name": "@przemekr95/quick-design-tokens",
  "version": "0.2.0",
  "description": "Modern design tokens library with TypeScript support",
  "type": "module",
  "main": "dist/js/cjs/index.js",
  "module": "dist/js/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/js/esm/index.js",
      "require": "./dist/js/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./scss": {
      "default": "./dist/scss/index.scss"
    },
    "./scss/core": {
      "default": "./dist/scss/core.scss"
    },
    "./scss/prPhoto": {
      "default": "./dist/scss/prPhoto.scss"
    },
    "./scss/prDev": {
      "default": "./dist/scss/prDev.scss"
    },
    "./css": {
      "default": "./dist/css/index.css"
    },
    "./css/core": {
      "default": "./dist/css/core.css"
    },
    "./css/prPhoto": {
      "default": "./dist/css/prPhoto.css"
    },
    "./css/prDev": {
      "default": "./dist/css/prDev.css"
    }
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "sideEffects": false,
  "scripts": {
    "build": "npm run clean && rollup -c && ts-node scripts/build-formats.ts",
    "dev": "rollup -c -w",
    "clean": "rimraf dist",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run lint && npm run build"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "design-tokens",
    "design-system",
    "css",
    "typescript",
    "react",
    "scss",
    "sass"
  ],
  "author": "przemekr95",
  "license": "MIT",
  "dependencies": {
    "lodash.merge": "^4.6.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "@types/lodash.merge": "^4.6.7",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint": "^8.16.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.75.5",
    "rollup-plugin-dts": "^4.2.2",
    "typescript": "^4.7.2"
  }
}